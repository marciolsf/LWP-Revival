@charset "utf-8";/* --------------------------------FILE NAME    ÅFbase.cssLAST UPDATE  ÅF2009.09.14-------------------------------- */body,div,p,dl,dt,dd,ul,ol,li,table,tr,th,td,h1,h2,h3,h4,h5,h6 {	margin: 0;	padding: 0;	vertical-align: baseline;}body {	background: #000000;	text-align: center;	color: #dddddd;}ul,li {	list-style-type: none;}img {	font-size: 0;	border: none;	vertical-align: bottom;}a {	color: #278bbe;}/* -- header -- */#header {	margin: 0 auto;}#header:after {	content: "";	clear: both;	height: 0;	display: block;	visibility: hidden;}h1,h2 {	float: left;}.copyright {	color: #666666;}/* -- mainphoto -- */#mainphoto {	background: #151515;	width: 100%;}#mainphoto img {	margin-bottom: 10px;}#mainphoto ul {	margin: 0 auto;}#mainphoto ul:after {	content: "";	clear: both;	height: 0;	display: block;	visibility: hidden;}#mainphoto li.caption {	float: left;}#mainphoto li.copyright {	float: right;}/* -- information -- */#information {	margin: 0 auto;}#information:after {	content: "";	clear: both;	height: 0;	display: block;	visibility: hidden;}#information #introduction {	float: left;	text-align: left;}#information #introduction:after {	content: "";	clear: both;	height: 0;	display: block;	visibility: hidden;}#information #introduction .magazine {	float: right;}#information #supplier {	float: right;}#information #supplier p {	text-align: left;}/* -- photoarea -- */#photo_area {	background: #151515;	margin: 0 auto;}#photo_area ul {	margin: 0 auto;}#photo_area li {	float: left;	margin-left: 10px;}#photo_area ul:after {	content: "";	clear: both;	height: 0;	display: block;	visibility: hidden;}#photo_area img {	margin-bottom: 10px;}#photo_area .caption {	margin-bottom: 5px;}#photo_area .line {	width: 100%;	height: 10px;	background: #000000;}/* -- footer -- */#footer {	margin: 0 auto;	text-align: right;	color: #666666;}/* [EOF] CSS */